# Параметры OpenAI/OpenRouter API
OPENAI_API_KEY=your-api-key-here
OPENAI_BASE_URL=https://openrouter.ai/api/v1
PROVIDER=groq

# Параметры генерации датасета
TOTAL_DIALOGS=500
MAX_BATCH_SIZE=20
OUTPUT_FILE=data/generated/llama-4-maverick-4.json
MODEL=meta-llama/llama-4-maverick

# Режимы работы (true/false)
PREVIEW_CONFIG=false   # не показывать пример конфигурации
DRY_RUN=false          # выполнять реальную генерацию
ASYNC_GENERATION=true  # включить асинхронный режим

# Параметры асинхронности
MAX_CONCURRENCY=10      # максимальное кол-во параллельных запросов 